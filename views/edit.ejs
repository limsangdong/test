      <%- include("_editor") -%>
      <div class="form-group row">
        <label class="col-form-label col-sm-2"></label>
        <div class="col-sm-10">
          <input type="submit" onclick="action='/edit/<%= model.idx %>'" value="업데이트" class="btn btn-default btn-warning" >
